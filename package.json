{
	"name": "lucida",
	"version": "2.0.0-37",
	"description": "A modular music downloader tool",
	"main": "build/index.js",
	"type": "module",
	"types": "build/index.d.ts",
	"scripts": {
		"tsc": "tsc",
		"build": "tsc",
		"watch": "tsc -w",
		"test": "node build/test.js",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"prepare": "husky"
	},
	"exports": {
		".": "./build/index.js",
		"./types": {
			"import": "./build/types.js",
			"types": "./build/types.d.ts"
		},
		"./streamers/*": "./build/streamers/*/main.js",
		"./streamers/*.js": "./build/streamers/*.js"
	},
	"files": [
		"build/**/*"
	],
	"keywords": [],
	"author": "hazycora",
	"repository": {
		"type": "git",
		"url": "https://git.gay/lucida/lucida"
	},
	"license": "OQL",
	"dependencies": {
		"blowfish-cbc": "^1.0.1",
		"image-size": "^1.1.1",
		"librespot": "^0.2.10",
		"undici": "^6.19.4",
		"xmldom-qsa": "^1.1.3"
	},
	"devDependencies": {
		"@types/node": "^20.11.17",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"husky": "^9.1.4",
		"prettier": "^3.2.5",
		"typescript": "^5.3.3"
	}
}
